{
  "AWSEBDockerrunVersion": 2,
  "authentication": {
    "bucket": "${dockerpie-auth}",
    "key": "${.dockercfg}"
  },
  "containerDefinitions": [
    {
      "essential": true,
      "image": {
        "name": "dockerpie.querypie.com/chequer.io/querypie-api:7.7.0",
        "update": true
      },
      "name": "querypie-api",
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 80
        }
      ]
    }
  ]
}
