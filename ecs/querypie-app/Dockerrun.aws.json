{
  "AWSEBDockerrunVersion": 2,
  "authentication": {
    "bucket": "${dockerpie-auth}",
    "key": "${.dockercfg}"
  },
  "containerDefinitions": [
    {
      "environment": [
        {
          "name": "PORT",
          "value": "3000"
        },
        {
          "name": "NODE_ENV",
          "value": "production"
        },
        {
          "name": "APP_ENV",
          "value": "master"
        },
        {
          "name": "REDIS_HOST",
          "value": "redis-server"
        },
        {
          "name": "REDIS_PORT",
          "value": "6379"
        },
        {
          "name": "REDIS_PASSWORD",
          "value": "PASSWORD"
        },
        {
          "name": "REDIS_EVENTKEY",
          "value": "master"
        },
        {
          "name": "REDIS_DB",
          "value": "0"
        }
      ],
      "essential": true,
      "image": {
        "name": "dockerpie.querypie.com/chequer.io/querypie-app:7.7.0",
        "update": true
      },
      "name": "querypie-app",
      "portMappings": [
        {
          "containerPort": 3000,
          "hostPort": 3000
        }
      ]
    },
    {
      "command": [
        "/opt/bitnami/scripts/redis/run.sh",
        "--maxmemory",
        "1024mb"
      ],
      "environment": [
        {
          "name": "ALLOW_EMPTY_PASSWORD",
          "value": "no"
        },
        {
          "name": "REDIS_DISABLE_COMMANDS",
          "value": "FLUSHDB,FLUSHALL"
        },
        {
          "name": "REDIS_PASSWORD",
          "value": "PASSWORD"
        },
        {
          "name": "REDIS_AOF_ENABLED",
          "value": "no"
        }
      ],
      "essential": true,
      "image": "docker.io/bitnami/redis:6.0-debian-10",
      "mountPoints": [
        {
          "containerPath": "/redis/data",
          "sourceVolume": "Redis_Data"
        }
      ],
      "name": "redis-server",
      "portMappings": [
        {
          "containerPort": 6379,
          "hostPort": 6379
        }
      ]
    }
  ],
  "family": "",
  "volumes": [
    {
      "host": {
        "sourcePath": "redis_data"
      },
      "name": "Redis_Data"
    }
  ]
}
